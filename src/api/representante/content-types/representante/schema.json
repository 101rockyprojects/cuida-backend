{
  "kind": "collectionType",
  "collectionName": "representantes",
  "info": {
    "singularName": "representante",
    "pluralName": "representantes",
    "displayName": "Representante",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "regex": "^[a-zA-Z\\s]*$",
      "maxLength": 80
    },
    "documento_identidad": {
      "type": "string",
      "required": true,
      "maxLength": 15,
      "unique": true,
      "regex": "^[0-9]+$"
    },
    "num_contacto": {
      "type": "string",
      "required": true,
      "maxLength": 10,
      "minLength": 10,
      "unique": true,
      "regex": "^[0-9]+$"
    },
    "refugio": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::refugio.refugio"
    },
    "usuario": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    }
  }
}
